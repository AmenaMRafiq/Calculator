package model;

/**
 * This enumeration class consists of four constants: NUMBER
 * SYMBOL, STRING, INVALID used to check the type of the 
 * Entry objects.
 * 
 * @author Amena Rafiq
 * @see    Entry
 */
public enum Type {
  NUMBER("Number"),
  SYMBOL("Symbol"),
  STRING("String"),
  INVALID("Invalid");
  
  private String type;
  
  private Type(String type) {
    this.type = type;
  }

  /**
   * Returns the string type corresponding to each Type constant.
   * 
   * @return type
   */
  public String toString() {
    return type;
  }
  
}
